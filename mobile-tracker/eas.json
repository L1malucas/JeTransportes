{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "develop": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "JAVA_HOME": "/opt/java/openjdk/jdk-17"
      }
    },
    "teste": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "hmg",
      "env": {
        "JAVA_HOME": "/opt/java/openjdk/jdk-17"
      }
    },
    "hmg": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "hmg",
      "env": {
        "JAVA_HOME": "/opt/java/openjdk/jdk-17"
      }
    },
    "main": {
      "distribution": "store",
      "node": "18.18.0",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "channel": "main",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}